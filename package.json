{
  "name": "w11k-angular-nest-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && node git-version.js",
    "nx": "nx",
    "start": "npm-run-all --parallel serve:*",
    "serve:client": "ng serve --project=client",
    "serve:backend": "ng serve --project=backend",
    "build": "ng build",
    "build:client": "ng build --project=client --prod",
    "build:backend": "ng build --project=backend --prod",
    "start:backend:prod": "node dist/apps/backend/main",
    "test": "ng test",
    "test:client": "ng test --project=client",
    "test:client:coverage": "ng test --code-coverage --project=client",
    "test:backend": "ng test --project=backend",
    "test:backend:coverage": "ng test --code-coverage --project=backend",
    "lint": "npm-run-all lint:*",
    "lint:client": "ng lint --project=client",
    "lint:backend": "ng lint --project=backend",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "migrate:generate": "./node_modules/.bin/ts-node --project apps/backend/tsconfig.app.json ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate:generate-dev": "cross-env DEV_MODE=true ./node_modules/.bin/ts-node --project apps/backend/tsconfig.app.json ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate:create": "./node_modules/.bin/ts-node --project apps/backend/tsconfig.app.json ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:run": "./node_modules/.bin/ts-node --project apps/backend/tsconfig.app.json ./node_modules/typeorm/cli.js migration:run",
    "migrate:run-dev": "cross-env DEV_MODE=true ./node_modules/.bin/ts-node --project apps/backend/tsconfig.app.json ./node_modules/typeorm/cli.js migration:run",
    "create-entities-index": "ts-node --pretty --project=tools/tsconfig.tools.json tools/create-entities-index.ts",
    "create-migrations-index": "ts-node --pretty --project=tools/tsconfig.tools.json tools/create-migrations-index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.0",
    "@angular/cdk": "^14.0.2",
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/material": "^14.0.2",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nrwl/angular": "14.1.9",
    "@w11k/git-info": "^1.0.2",
    "bootstrap": "^5.1.3",
    "fs": "^0.0.1-security",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.34",
    "typescript": "4.6.4",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@angular/language-service": "14.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.1.9",
    "@nrwl/cypress": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/nest": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@types/jest": "27.4.1",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@w11k/eslint-config": "^0.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^7.3.0",
    "dotenv": "10.0.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-rxjs": "^5.0.2",
    "fs-extra": "6.0.1",
    "git-describe": "4.0.4",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "npm-run-all": "^4.1.5",
    "nx": "14.1.9",
    "prettier": "2.6.1",
    "ts-jest": "27.1.4",
    "ts-node": "~9.1.1",
    "typescript": "~4.6.2"
  }
}
